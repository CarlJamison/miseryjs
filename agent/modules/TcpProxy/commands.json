{
    "tcp-proxy" : {
        "depends": "TcpProxy",
        "agent_alias": ["extension create-tcp-server {*}"],
        "summary": "Sets up a TCP Proxy",
        "usage": "tcp-proxy {target host} {target port} {server port}"
    },
    "get-tcp-proxies" : {
        "agent_alias": ["extension get-tcp-servers"],
        "summary": "Gets all existing TCP Proxies"
    },
    "close-tcp-proxy" : {
        "agent_alias": ["extension close-tcp-server {*}"],
        "summary": "Closes a TCP Proxy",
        "usage": "tcp-proxy {server port}"
    }
}
